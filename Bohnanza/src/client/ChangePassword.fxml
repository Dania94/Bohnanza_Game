<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="280.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/8.0.211" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ChangePasswordController">
	<children>
		<PasswordField fx:id="confirmPassword" layoutX="182.0" layoutY="128.0" promptText="Passwort bestaetigen">
			<font>
				<Font size="13.0" />
			</font>
		</PasswordField>
		<PasswordField fx:id="currentPassword" layoutX="182.0" layoutY="45.0" promptText="altes Passwort">
			<font>
				<Font size="13.0" />
			</font>
		</PasswordField>
		<PasswordField fx:id="newPassword" layoutX="182.0" layoutY="87.0" promptText="neues Passwort">
			<font>
				<Font size="13.0" />
			</font>
		</PasswordField>
		<Label layoutX="37.0" layoutY="49.0" text="altes Passwort : ">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<Label layoutX="37.0" layoutY="91.0" text="neues Passwort :">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<Label layoutX="37.0" layoutY="132.0" text="Passwort bestaetigen :">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<Button fx:id="changePasswordButton" layoutX="41.0" layoutY="213.0" mnemonicParsing="false" onAction="#changePasswordButtonClicked" text="Passwort aendern" />
		<Button fx:id="cancelButton" layoutX="208.0" layoutY="213.0" mnemonicParsing="false" onAction="#cancelButtonClicked" prefHeight="26.0" prefWidth="126.0" text="Abbrechen" />
	</children>
</AnchorPane>
